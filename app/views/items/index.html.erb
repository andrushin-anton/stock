<% content_for :title, 'Financial Instruments | Stock2.Watch' %>
<br>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Symbol</th>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.symbol %></td>
        <td><%= item.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div class="paging">
    <%= will_paginate @items, renderer: BootstrapPagination::Rails %>
</div>